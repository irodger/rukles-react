n-delay: 4s;\n    }\n\n    &::after {\n      content: '';\n      position: absolute;\n      width: 35%;\n      height: 25%;\n      background: rgba(#fff,0.7);\n      border-radius: 50%;\n      top: 17%;\n      left: 15%;\n      transform: rotate(-45deg);\n    }\n  }\n\n  &__liquid {\n    width: inherit;\n    height: inherit;\n    background: #552f4e;\n    border-radius: 50% 50% 40% 40%/25% 25% 60% 60%;\n    transform: scaleY(0.5) scaleX(0.93);\n    transform-origin: 50% 89%;\n    animation: 3s fillBottle linear;\n\n    &::after {\n      content: '';\n      width: 230px;\n      height: 100px;\n      border-radius: 70% 70% 50% 50% / 80% 80% 100% 100%;\n      background: #2d2e44;\n      left: calc(50% - 115px);\n      position: absolute;\n      margin-top: 3px;\n      animation: 3s waves infinite ease;\n    }\n  }\n}\n\n@keyframes fillBottle {\n  0% {\n    transform: scaleY(0.1) scaleX(0.8);\n  }\n  50% {\n    transform: scaleY(0.5) scaleX(0.93);\n  }\n  100% {\n    transform: scaleY(0.5) scaleX(0.93);\n  }\n}\n\n@keyframes flyaway {\n  0% {\n    transform: translateY(0px) scale(1);\n  }\n  10% {\n    opacity: 1;\n  }\n  50%, 100% {\n    transform: scale(1.5) translateY(-150px);\n    opacity: 0;\n  }\n}\n\n@keyframes waves {\n  0%,100% {\n    transform: scaleY(1.2);\n  }\n  50% {\n    transform: scaleY(1);\n  }\n}\n\n@keyframes type {\n  0%, 100% {\n     width: 0;\n  }\n  50% {\n    width: 22px;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"adb63ffd65d954fea7cbae5ce2de7a27","cacheData":{"env":{}}}